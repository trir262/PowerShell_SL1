              <TableColumnItem>
                <PropertyName>cred_port</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <ScriptBlock>
                  switch ($_.db_type) {
                  1 { 'MySQL' }
                  2 { 'MSSQL' }
